{
	"@context": {
    	"@vocab": "https://raw.githubusercontent.com/IATA-Cargo/ONE-Record/master/schema/waybill-line/",
    	"type": "@type",
    	"id": "@id"
    },
    "id": {
        "description": "URI of this logistics object",
         "required": true,
         "type": "text"
    },
    "type":  {
        "description": "Type of losgistics object, e.g. air waybill, booking, etc",
         "required": true,
         "type": "text"
    },
    "typeCode":  {
        "description": "Commonly useed codes for document type, e.g. jfwb, jfhl etc. Leave nlank if unknown",
        "type": "text"
    },
    "issuedBy":  {
        "description": "Internet of logistics identity code of issuing party.",
        "required": true,
        "type": "text"
    },
    "key":  {
        "description": "JSON field name of code that identifies this logistics object, e.g. 'waybill number', 'UPID' etc.",
        "required": true,
        "type": "text"
    },
    "waybill-line":
      {
      "LineID": {
         "description": "Unique line identifer",
         "required": true,
         "type": "numeric"
      },
      "Units": {
         "description": "Units of measure",
         "required": true,
         "type": "undefined"
      },
      "Currency": {
         "description": "Currency details",
         "required": true,
         "type": "undefined"
      },
      "Seller": {
         "description": "Seller company details  - e.g. the party issuing the invoice",
         "type": "undefined"
      },
      "Shipper": {
         "description": "Shipper company details - e.g. the party shipping the items ",
         "type": "undefined"
      },
      "Buyer": {
         "description": "Buyer company details - e.g. the party paying the invoice",
         "type": "undefined"
      },
      "Consignee": {
         "description": "Consignee company details - e.g. the party receiving the items",
         "type": "undefined"
      },
      "Freight Forwarder": {
         "description": "Freight Forwarder company details ",
         "type": "undefined"
      },
      "Consolidator": {
         "description": "Consolidator company details - e.g. the party consolidating multiple pieces",
         "type": "undefined"
      },
      "OtherParty": {
         "description": "Other party company details - e.g. the party to be notified",
         "type": "undefined"
      },
      "ReferenceDocument": {
         "description": "Reference documents details ",
         "type": "undefined"
      },
      "Incoterms": {
         "description": "Incoterms details ",
         "type": "undefined"
      },
      "Insurance": {
         "description": "Insurance details",
         "type": "undefined"
      },
      "HandlingDetails": {
         "description": "Handling details ",
         "type": "undefined"
      },
      "CustomsDetails": {
         "description": "Customs details ",
         "type": "undefined"
      },
      "SecurityDetails": {
         "description": "Security details ",
         "type": "undefined"
      },
      "FreightTerms": {
         "description": "Freight terms details",
         "type": "undefined"
      },
      "ChargeDeclaration": {
         "description": "Charge declaratrion details ",
         "type": "undefined"
      },
      "LogisticsServiceCharge": {
         "description": "Logistics Service Charge details ",
         "type": "undefined"
      },
      "OtherCharges": {
         "description": "Other Charge details ",
         "type": "undefined"
      },
      "Quantity": {
         "PieceCount": {
            "description": "Number of pieces / packages / ULDs",
            "type": "numeric"
         },
         "SLAC": {
            "description": "Shipper's Load and Count - e.g. count of Items",
            "type": "numeric"
         }
      },
      "DimensionsPerPiece": {
         "NoDimensionAvailable": {
            "description": "NDA (No Dimensions Available) indicator",
            "type": "text"
         },
         "Width": {
            "description": "Width of piece / package / ULD",
            "type": "numeric"
         },
         "Length": {
            "description": "Length of piece / package / ULD",
            "type": "numeric"
         },
         "Height": {
            "description": "Height of piece / package / ULD",
            "type": "numeric"
         },
         "Volume": {
            "description": "Volume of piece / package / ULD",
            "type": "numeric"
         }
      },
      "Weight": {
         "GrossWeight": {
            "description": "Gross weight of piece / package / ULD - total for the waybill line",
            "type": "numeric"
         },
         "VolumeWeightConversionFactor": {
            "description": "Volume to weight conversion factor",
            "type": "numeric"
         },
         "ChargeableWeight": {
            "description": "Chargeable weight - total for the waybill line",
            "type": "numeric"
         }
      },
      "DensityGroup": {
         "description": "Density group for the goods",
         "type": "text"
      },
      "DeckPosition": {
         "description": "Position of the shipment in the aircraft - e.g. lower or main deck ",
         "type": "text"
      },
      "Goods": {
         "GoodsDescription": {
            "description": "Generic text description of the goods ",
            "type": "text"
         },
         "OriginCountryCode": {
            "description": "Origin country ISO code of the goods",
            "type": "text"
         },
         "CommodityCode": [{
            "Issuer": {
               "description": "Issuer of commodity classification codes - e.g. Brussels Tariff Nomenclature, EU Harmonized System Code, UN Standard...International Trade Classification",
               "type": "text"
            },
            "Code": {
               "description": "Commodity code",
               "type": "text"
            },
            "Name": {
               "description": "If no CommodityCode provided, name of commodity",
               "type": "text"
            }
         }]
      },
      "Rating": {
         "RatingTypeIndicator": {
            "description": "Transportation rating type - e.g. F for Face, P for Published, or A for Actual Rating detail",
            "type": "text"
         },
         "CommodityItemNumber": {
            "description": "Commodity identifier on which the rate class code is applied",
            "type": "text"
         },
         "RateClassCode": {
            "description": "Rate category code",
            "type": "text"
         },
         "RateChargeDiscountAmount": {
            "description": "Rate, charge or discount amount",
            "type": "numeric"
         },
         "RCP": {
            "description": "City code of Rate Combination Point (RCP), the point over which sector rates are combined",
            "type": "text"
         },
         "ULDRateClassType": {
            "description": "Unit Load Device rate class type",
            "type": "text"
         },
         "RateClassCodeBasis": {
            "description": "Basis on which the Rate Class Code is applicable",
            "type": "text"
         },
         "ClassRatingPercentage": {
            "description": "Rating class percentage",
            "type": "numeric"
         },
         "RateReferenceCode": {
            "description": "A unique code to indicate the applicable rate",
            "type": "text"
         },
         "RateReferenceType": {
            "description": "Type of rate - e.g. contract, adhoc, etc.",
            "type": "text"
         },
         "SubTotalLineAmount": {
            "description": "Total charge for this line item",
            "type": "numeric"
         }
      },
      "PieceDetail": [{
      }]
   }
}