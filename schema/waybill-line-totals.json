{
	"@context": {
    	"@vocab": "https://raw.githubusercontent.com/IATA-Cargo/ONE-Record/master/schema/waybill-line-totals/",
    	"type": "@type",
    	"id": "@id"
    },
    "id": {
        "description": "URI of this logistics object",
         "required": true,
         "type": "text"
    },
    "type":  {
        "description": "Type of losgistics object, e.g. air waybill, booking, etc",
         "required": true,
         "type": "text"
    },
    "typeCode":  {
        "description": "Commonly useed codes for document type, e.g. jfwb, jfhl etc. Leave nlank if unknown",
        "type": "text"
    },
    "issuedBy":  {
        "description": "Internet of logistics identity code of issuing party.",
        "required": true,
        "type": "text"
    },
    "key":  {
        "description": "JSON field name of code that identifies this logistics object, e.g. 'waybill number', 'UPID' etc.",
        "required": true,
        "type": "text"
    },
    "waybill-line-totals":
      {
      "Units": {
         "description": "Units of measure",
         "required": true,
         "type": "undefined"
      },
      "Currency": {
         "description": "Currency details",
         "required": true,
         "type": "undefined"
      },
      "TotalLineCount": {
         "description": "Count of WAYBILL_LINE",
         "type": "numeric"
      },
      "TotalPieceCount": {
         "description": "Sum of WAYBILL_LINE PieceCount",
         "type": "numeric"
      },
      "TotalSLAC": {
         "description": "Sum of WAYBILL_LINE SLAC",
         "type": "numeric"
      },
      "TotalVolume": {
         "description": "Sum of WAYBILL_LINE Volume",
         "type": "numeric"
      },
      "TotalGrossWeight": {
         "description": "Sum of WAYBILL_LINE GrossWeight",
         "type": "numeric"
      },
      "TotalChargeableWeight": {
         "description": "Sum of WAYBILL_LINE ChargeableWeight",
         "type": "numeric"
      },
      "TotalLineAmount": {
         "description": "Sum of WAYBILL_LINE SubTotalLineAmount",
         "type": "numeric"
      }
   }
}