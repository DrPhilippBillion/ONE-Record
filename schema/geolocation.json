{
	"@context": {
    	"@vocab": "https://raw.githubusercontent.com/IATA-Cargo/ONE-Record/master/schema/geolocation/",
    	"type": "@type",
    	"id": "@id"
    },
    "id": {
        "description": "URI of this logistics object",
         "required": true,
         "type": "text"
    },
    "type":  {
        "description": "Type of losgistics object, e.g. air waybill, booking, etc",
         "required": true,
         "type": "text"
    },
    "typeCode":  {
        "description": "Commonly useed codes for document type, e.g. jfwb, jfhl etc. Leave nlank if unknown",
        "type": "text"
    },
    "issuedBy":  {
        "description": "Internet of logistics identity code of issuing party.",
        "required": true,
        "type": "text"
    },
    "key":  {
        "description": "JSON field name of code that identifies this logistics object, e.g. 'waybill number', 'UPID' etc.",
        "required": true,
        "type": "text"
    },
    "geolocation":
      {
      "LatitudeDecimal": {
         "description": "none",
         "type": "numeric"
      },
      "LongitudeDecimal": {
         "description": "none",
         "type": "numeric"
      },
      "AltitudeMetric": {
         "description": "none",
         "type": "numeric"
      },
      "DockElevationMetric": {
         "description": "none",
         "type": "numeric"
      },
      "DockApproachInfo": {
         "description": "none",
         "type": "text"
      },
      "DockFloor": {
         "description": "none",
         "type": "text"
      },
      "DockID": {
         "description": "none",
         "type": "text"
      },
      "DockOtherInfo": {
         "description": "none",
         "type": "text"
      },
      "Note": {
         "description": "none",
         "type": "text"
      }
   }
}