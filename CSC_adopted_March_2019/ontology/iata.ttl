#####################################################################################
# schema of the AirFreightPounch                                                    #
#####################################################################################

###### class definitions ######

<AirFreightPouch> a rdfs:Class ;
   rdfs:label "The collection of all air freight documents currently supported by ONE Record"@en .

###### property definitions ######

<invoice> a rdfs:Property ;
   rdfs:label "Invoice is one of the main IATA freight documents"@en ;
   schema:domainIncludes <AirFreightPouch> ;
   schema:rangeIncludes <Invoice> .

<packingList> a rdfs:Property ;
   rdfs:label "Packinglist is one of the main IATA freight documents"@en ;
   schema:domainIncludes <AirFreightPouch> ;
   schema:rangeIncludes <PackingList> .

<shipperLetterOfInstruction> a rdfs:Property ;
   rdfs:label "Shipperletterofinstruction is one of the main IATA freight documents"@en ;
   schema:domainIncludes <AirFreightPouch> ;
   schema:rangeIncludes <ShipperLetterOfInstruction> .

<houseWaybill> a rdfs:Property ;
   rdfs:label "Housewaybill is one of the main IATA freight documents"@en ;
   schema:domainIncludes <AirFreightPouch> ;
   schema:rangeIncludes <HouseWaybill> .

<booking> a rdfs:Property ;
   rdfs:label "Booking is one of the main IATA freight documents"@en ;
   schema:domainIncludes <AirFreightPouch> ;
   schema:rangeIncludes <Booking> .

<airWaybill> a rdfs:Property ;
   rdfs:label "Airwaybill is one of the main IATA freight documents"@en ;
   schema:domainIncludes <AirFreightPouch> ;
   schema:rangeIncludes <AirWaybill> .

<houseManifest> a rdfs:Property ;
   rdfs:label "Housemanifest is one of the main IATA freight documents"@en ;
   schema:domainIncludes <AirFreightPouch> ;
   schema:rangeIncludes <HouseManifest> .

<commonObjects> a rdfs:Property ;
   rdfs:label "Commonobjects is one of the main IATA freight documents"@en ;
   schema:domainIncludes <AirFreightPouch> ;
   schema:rangeIncludes <CommonObjects> .


